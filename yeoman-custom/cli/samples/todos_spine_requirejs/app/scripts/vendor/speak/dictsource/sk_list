// Pronouns
sa       $u+            // reflexive
si       $u+            // reflexive
ho  $u+

// Prepositions
o        o_ $u $brk $combine
k        k  $brk         // at,on,to
v        v_  $brk        // at,in,into
z        z_  $brk        // from,of
na       $u     $combine    // at,for,into
po       $pause $combine    // about,after
pod      $pause $combine    // under
s        s_ $pause       // with
so       $pause $combine    // with
bezo     $pause $combine    // without
bez      $pause $combine    // without
pri      $pause $combine    // about,beside,at

// Conjunctions
a        $u $pause      // and
alebo    $u $pause      // or
inak     $u $pause      // or
síce     $u $pause      // or
ale      $pause         // however
však     $pause         // but
okrem    $pause         // except
mimo     $pause         // except
čo       $u $pause      // which,whatever
aj       $u $pause      // even
že       $u $pause      // if,that

ako      $u $pause      // as

// Negative
nie      $u


// Auxillary verbs

byť      $u    // to be
som      $u
si       $u
je       $u
sme      $u
ste      $u
sú       $u
bol      $u+   // been


// treat as letters if at the end of the clause
a        a:    $atend    // letter-a
k        ka:   $atend    // letter-k
o        o:    $atend    // letter-o
s        es    $atend
v        ve:   $atend
z        zet   $atend

// Exceptions

// syllabic l
dlh	dl-h
stĺp	stl:p
plch	pl-x
čln	tSl-n

// don't soften (d,n,t) before e,i
