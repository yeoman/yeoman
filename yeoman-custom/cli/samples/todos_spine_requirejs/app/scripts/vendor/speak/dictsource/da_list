_4	f'i:V
_5	f'Em
_6	s'Egs
_7	s'yw
_8	'O:d@
_9	n'i
_10	t'i
_11	'Elv@
_12	t'Vl
_13	tR'&#d@-n // PB & changed to &#
_14	fj'ord@-n
_15	 f'emt@n
_16	s'ajsd@-n
_17	s'Wd@-n // PP Y changed to W
_18	'&d@-n  // PB a changed to &
_19	n'ed@-n
_2X	t'y:w
_3X	tR'&#Dv@               //PB [tR'ADv@] changed to [tR'&#Dv@]
_4X	 f'W:V                 //PB [f'W:*3] changed to [ f'W:V]
_5X	h&l't*es               // PB [a] changed to [&]
_6X	t*'es
_7X	h&lfj'&rs               // PV a changed to &
_8X	f'irs
_9X	h&lf'Ems // PB a changed to &
_0C	h'un*@:D
_1C  ed||h'unR@:D // PB addet 'et hundrede'
_0M1	t'u:s@-n
_1M1    ed||t'u:s@-n      // PB added 'et' to tusind
_1M2	'e:n||milij'o:n
_0M2	milij'o:n3
_1M3	'e:n||milij'A:d
_0M3	milij'A:d3
_1M4	'e:n||bilij'o:n
_0M4	bilij'o:n3

_0and	V
_dpt	_k,Vm&:

// PB a number followed by dot
// ordinals are followed by a dot in Danish
_ord      3  // 13. (trettende)

// exceptions for ordinals
_1o fWrsd3 // 1. første
_2o &n@n // 2. anden
_3o trEdj3 // 3. tredje
_4o fjeV // 4. fjerde
_5o fEmt3 // 5. femte
_6o sj&d3 // 6. sjette
_7o syvn3 // 7. syvende
_8o Vtn3 // 8. ottende
_9o ni@n3 // 9. niende
_10o ti@n3 // 10. tiende
_11o Elfd3 // 11. elvte/ellevete
_12o tVlfd3 // 12. tolvte

_2Xo   t'yv@n@ // 25.
_3Xo   tr'&#ft@ //36.
_4Xo   fWV'tyv@n@ // 42.
_5Xo   halt*'Esenstyv@n@ // 52.
_6Xo   tr'Esenstyv@n@ // 62.
_7Xo    h&lfj'&rsenstyv@n@ // 72.
_8Xo    firsenstyv@n@ // 82.
_9Xo   half'Emsenstyv@n@ // 95.

// function words

den	dEn	$u
en	e:n	$u
et	et	$u
her $u
kun $u
vel $u+
da $u

// conjunctions
og	V	$u+ $pause  // and
men		$pause	// but
eller	// or
mens		$pause	// while
for	fV
fordi	fVd'i $pause	// because - PB: changed stress
når	nVr		$pause	// while
der	dEr	$u+ $u    // PB changed from [dA]
hvor	v'Vr	// where
hvorfor	vVf'V	// why - final r deleted

// prepositions
på		pO	$u
til			$u+	// toward
fra			$u+	// from
ved			$u+	// at
om			$u+ $brk	// about, within
med			$u+	// with
af      &  	// PB changed from [a?] to [&] - sound too short in a sentence
            // Also removed $u from 'af' and other words. They disappeared.
ad      &:  // PB added 'ad' 
ad &D $atend $sentence // PB "Det må ikke skille os ad."
at		&: $u // PB changed from [at] to [&:]
hos			// at - PB: removed [$u+]
som	sVm	 $u   // which / that - PB: removed [$u+]

// pronouns
jeg	jai	$u+	// I 
du du $u
de	di	$u
han	$u
hun	$u
det	de $u
dét de
vi	vi $u
i	_i $u+
mig	maj	$u	// me
dig	daj	$u+
dem	$u
ham	$u
hende	$u
sig	$u
os	Vs $u+
jer	$u
denne	$u+	// this
dette	$u+
disse	$u+	// these
hvis ves   $u+  // PB - whose

