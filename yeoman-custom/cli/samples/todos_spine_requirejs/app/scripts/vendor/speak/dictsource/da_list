_mcn	m'akr,0n
_ogo	'0g0n,Ek
_rng	ring
_stk	str'ouk
_tld	tild@

_smc	sm'O:lk,ap
_rev	riv'3:sd
_tur	t'3:nd
_bar	_b'A@
_rfx	rtroflEks
_crl	k'3:l
_hok	h'uk


// character names
_cap	stoVt||boustaw
_?A	boustaw
_??	symbol
_#9	tab
_#32	blank

*	'asd&r,isg	$max3
%	pRos'end	$max3
@	sn,&:bel'&:
/	sgRVsdrai	$max3
\	'Vmvendsgr,VsdRai $max3
_,	k'oma:
_;	se:mi:ko:lVn
_:	k'o:lVn
_.	pONtOm                  // PN u => O
_?	spWrgsmVlstajn
_!	udRVbstejn
_-	binnesdrai
__	b'Onsdrai
&	Vw
£	pun?
€	euro
© kopyrajt
÷ divid'eReD||meD 
=	l'imeD
#	n'OmV
§	paragr'af
×	g'aNe
ª	feminin||Wdin'al
º	m'askulin||Wdin'al
®	registr'eret||varemerke
¼	en||kvart
¢	s'ents
¥	j'en:
µ	my
