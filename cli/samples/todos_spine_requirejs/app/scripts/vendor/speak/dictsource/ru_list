// characters
№	 номер	$text


// prepositions
в	v $u         // to,in,at
от	  $u         // from
с	s $u         // with,from,since
у	u $u         // by
на        $u         // on
для	  $u         // for


// conjunctions
и	i $u $pause    // and
а	 $u $pause    // and
а	 $atend
но	$u $pause    // but
ибо	$u $pause    // for

ибо	$u $pause    // if
хотя	$2 $pause       // although
чтобы	$pause       // in order that


// pronouns
я       $u           // I


// questions
что     Sto  $u $pause     // what, that


// misc.
не	$u           // negative


