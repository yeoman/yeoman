// Pronouns
mi       $u
ti       $u
se       $u
si       $u


// Prepositions
// the $combine prepositions should also be included in cs_rules
// as prefixes, so "na-" (for example) is removed from the combined
// word before the pronuciation of the main word is determined.

skrz     $u $brk $combine    // though
skrze    $u $pause        // through
pro      $u $pause $combine  // for
na       $u $brk $combine    // to/for/on
o        $u $pause $combine  // around/about
při      $u $pause $combine  // into,in,around
v        v_ $brk          // in
po       $u $pause $combine  // after,around
za       $u $pause $combine  // behind
před     $u $pause $combine  // in front of
mezi     $pause           // between
pod      $u $pause $combine  // below
pode     $u $pause        // below
s        s_ $pause        // with
nad      $u $pause $combine  // above
nade     $u $pause        // above

k        $brk             // towards
proti    $u $pause        // against
díky     $pause           // thanks to
naproti  $pause           // opposite

během    $pause           // during
podle    $pause           // according to
dle      $u $pause $combine  // along
vedle    $pause           // beside
kolem    $pause           // around
okolo    $pause           // around
do       $u $brk $combine    // to
od       $u $brk $combine    // away from
ode      $u $brk          // away from
z        s_ $brk          // out of/from
ze       $u $brk $combine    // out of/from
bez      $pause $combine     // without
beze     $pause           // without
místo    $pause           // instead of

i        $u $brk    // ???

// Conjunctions
a        $u $brk $pause      // and
nebo     $u $pause      // or
však     $pause         // but
krom     $pause         // except
ale      $pause         // however
ač       $pause         // although
leč      $pause         // unless
sice     $pause         // although
že       $u $pause      // if,that
mimo     $pause         // except
což      $pause         // which
ani      $pause         // neither,nor

// Negative


// Auxillary verbs
být      $u     // to be
jsem     $u     // am
jsi      $u
je       $u
jsme     $u
jste     $u
jsou     $u

byl      $u     // (past participle)
byla     $u
bylo     $u
byli     $u
byly     $u


budu     $u     // (future)
budeš    $u
bude     $u
budeme   $u
budete   $u
budou    $u

bych     $u     // (conditional)
bys      $u
by       $u
bychom   $u
byste    $u
by       $u

mám      $u+     // have
máš      $u+
má       $u+
máme     $u+
máte     $u+
mají     $u+
měl      $u+

chci     $u+     // want
chceš    $u+
chce     $u+
chceme   $u+
chcete   $u+
chtějí   $u+

// treat as letters if at the end of the clause
a        a:    $atend    // letter-a
i        i:    $atend    // letter-i
k        ka:   $atend    // letter-k
o        o:    $atend    // letter-o
s        es    $atend
v        ve:   $atend
z        zet   $atend


// Abbreviations
// if a word has no vowels (or "r") then it will
// automatically be spoken as individual letters

e.g	napR^

