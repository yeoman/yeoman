
// Kurdish translation rules
// This file is UTF-8 encoded

.L01 î iy ê ew ey   // front vowels

.group a
        a        a

     @) a (_S1   %a
     @) an (_S2  %an

.group b
        b        b

.group c
        c        dZ

.group ç
        ç        tS

.group d
        d        d

.group e
        e        E
        e (_     E#
        ew       eU
        ey       eI

.group ê
        ê        e

     @) ê (_S1   %e
     @) ên (_S2  %en


.group f
        f        f

